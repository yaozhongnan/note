{
  "title": "note",
  "author": "yaozhongnan",
  "description": "a study note for myself",
  "language": "zh-hans",
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "insert-logo",
    "code",
    "github",
    "advanced-emoji",
    "splitter",
    "ancre-navigation",
    "pageview-count",
    "popup",
	"hide-element",
	"custom-favicon",
	"tbfed-pagefooter"
  ],
  "pluginsConfig": {
    "insert-logo": {
      "url": "/note/images/logo.jpg",
      "style": "background: none; max-height: 30px; min-height: 30px"
    },
    "github": {
      "url": "https://github.com/yaozhongnan/note"
    },
	"hide-element": {
		"elements": [".gitbook-link"]
	},
	"favicon": "/note/images/favicon.ico",
	"tbfed-pagefooter": {
		"copyright":"Copyright © yaozhongnan All Right Reserved ",
		"modify_label": "该文章修订时间：",
		"modify_format": "YYYY-MM-DD HH:mm:ss"
	}
  }
}
